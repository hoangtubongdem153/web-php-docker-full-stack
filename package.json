{
  "homepage": ".",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "sitemap": "next-sitemap --config next-sitemap.config.js",
    "s3:upload": "node ./deployment/s3",
    "prepare": "husky install",
    "vss-build": "chmod +x scripts/build.sh && scripts/build.sh"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.1",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "next": "^12.3.1",
    "next-auth": "^4.22.1",
    "next-sitemap": "^2.5.20",
    "videojs-contrib-quality-levels": "^3.0.0",
    "videojs-hls-quality-selector": "^1.1.4",
    "videojs-sprite-thumbnails": "^0.6.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@types/node": "^18",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --config ./prettier.config.js --write"
    ],
    "**/*.{css,scss,md,html,json}": [
      "prettier --config ./prettier.config.js --write"
    ]
  }
}
